{
	"name": "@loopdrape/js-libraries",
	"version": "2.5.0",
	"description": "",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/loopdrape/js-libraries.git"
	},
	"main": "./src/index.js",
	"author": "hys kohsuke",
	"license": "MIT",
	"directories": {
		"production": "",
		"test": "_test",
		"pageDir": ""
	},
	"browserslist": {
		"production": [
			">0.2% in JP",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.8.3",
		"@babel/preset-env": "^7.8.3",
		"autoprefixer": "^9.7.4",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.0.6",
		"copy-webpack-plugin": "^5.1.1",
		"core-js": "^3.6.4",
		"cross-env": "^6.0.3",
		"css-loader": "^3.4.2",
		"ejs": "^2.7.1",
		"ejs-html-loader": "^4.0.1",
		"eslint": "^5.0.0",
		"eslint-config-recommended": "^4.0.0",
		"eslint-loader": "^3.0.3",
		"file-loader": "^4.2.0",
		"globule": "^1.2.1",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.9.0",
		"module-alias": "^2.2.2",
		"node-sass": "^4.13.1",
		"node-sass-once-importer": "^5.3.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-loader": "^3.0.0",
		"regenerator-runtime": "^0.13.3",
		"sass-loader": "^8.0.2",
		"stylelint": "^10.0.0",
		"stylelint-config-standard": "^19.0.0",
		"stylelint-config-standard-scss": "^1.1.0",
		"stylelint-order": "^4.0.0",
		"stylelint-scss": "^3.13.0",
		"stylelint-webpack-plugin": "^1.2.1",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.10.1"
	},
	"dependencies": {
		"normalize.css": "^8.0.1"
	},
	"_moduleAliases": {
		"@@build": "build_modules",
		"@@assets": "src/assets",
		"@@libraries": "src/libraries"
	},
	"eslintIgnore": [
		"node_modules/",
		"dist/"
	],
	"scripts": {
		"start": "webpack-dev-server",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "yarn lint:js && yarn lint:style",
		"lint:js": "eslint --fix --quiet",
		"lint:style": "stylelint \"**/*.(sa|sc|c)ss\" --fix --quiet",
		"watch": "webpack --colors -w",
		"build:log": "webpack --colors > build.log",
		"build": "yarn build:log",
		"build:test": "cross-env NODE_ENV=test webpack --colors",
		"build:prod": "cross-env NODE_ENV=production webpack --colors",
		"dist": "yarn build:prod && node ./build_modules/updSourceMap",
		"build:lib-dev": "cross-env B_TYPE=libraries webpack --colors",
		"build:lib-prod": "cross-env NODE_ENV=production B_TYPE=libraries webpack --colors",
		"dist:lib": "yarn build:lib-dev && yarn build:lib-prod && node ./build_modules/updSourceMap"
	}
}
